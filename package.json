{
  "private": false,
  "name": "raind",
  "version": "1.1.6",
  "description": "a react components lib named rain-ui",
  "homepage": "https://rain-ui.liufashi.top",
  "keywords": [
    "raind",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liufashi-Mr/rain-ui"
  },
  "author": {
    "name": "fashi",
    "email": "1325178274@qq.com",
    "url": "https://blog.liufashi.top"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "rm -rf ./dist &&  npm run build:esm & npm run build:umd & npm run build:cjs",
    "build:esm": "webpack --config-name esm",
    "build:cjs": "webpack --config-name cjs",
    "build:umd": "webpack --config-name umd",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --cache --cache-location './node_modules/.cache/.eslintcache' --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:style": "stylelint {src,docs}/**/*.{less,css} --fix",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepare": "husky install"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/index.js",
  "types": "dist/types/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "npm run lint:fix"
    ],
    "*.{less,css}": [
      "prettier --write",
      "npm run lint:style"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons": "^4.8.0",
    "classnames": "^2.3.2"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.10",
    "@types/react-transition-group": "^4.4.5",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "^5.0.6",
    "babel-loader": "^9.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.27.2",
    "css-loader": "^6.7.2",
    "dumi": "1",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.7.1",
    "react-color": "^2.19.3",
    "react-transition-group": "^4.4.5",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
